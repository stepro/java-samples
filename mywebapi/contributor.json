{
    "from": "openjdk:8-jre-slim",
    "fromBuild": "maven:3.5-jdk-8-slim",
    "containerPort": "8080",
    "copyProjectFiles": [
        "pom.xml ."
    ],
    "runBuildCommands": [
        "mvn package -Dmaven.test.skip=true"
    ],
    "copyBuiltFiles": [
        "target/mywebapi-0.1.0.jar ."
    ],
